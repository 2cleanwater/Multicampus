<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	

<bean id="shape1" class="shape.Triangle">
	<constructor-arg index="0" type="int" value="7"/>
	<constructor-arg index="1" type="int" value="5"/>
	<constructor-arg index="2" type="String" value="Blue"/>
</bean>

<bean id="shape2" class="shape.Rectangle">
	<constructor-arg index="0" type="int" value="4"/>
	<constructor-arg index="1" type="int" value="6"/>
	<constructor-arg index="2" type="String" value="Blue"/>
</bean>

<bean id="shape3" class="shape.Triangle">
	<constructor-arg index="0" type="int" value="6"/>
	<constructor-arg index="1" type="int" value="7"/>
	<constructor-arg index="2" type="String" value="Red"/>
</bean>

<bean id="shape4" class="shape.Rectangle">
	<constructor-arg index="0" type="int" value="8"/>
	<constructor-arg index="1" type="int" value="3"/>
	<constructor-arg index="2" type="String" value="Red"/>
</bean>

<bean id="shape5" class="shape.Triangle">
	<constructor-arg index="0" type="int" value="9"/>
	<constructor-arg index="1" type="int" value="8"/>
	<constructor-arg index="2" type="String" value="white"/>
</bean>

<bean id="shape6" class="shape.Rectangle">
	<constructor-arg index="0" type="int" value="5"/>
	<constructor-arg index="1" type="int" value="7"/>
	<constructor-arg index="2" type="String" value="white"/>
</bean>

</beans>
	